{
    "app": {
        "bundleName": "com.huawei.codelab",
        "vendor": "huawei",
        "apiVersion": {
            "compatible": 7,
            "target": 7,
            "releaseType": "Beta2"
        },
        "version": {
            "code": 1000000,
            "name": "1.0.0"
        }
    },
    "deviceConfig": {
        
    },
    "module": {
        "name": ".MyApplication",
        "deviceType": [
            "phone",
            "tablet",
            "car"
        ],
        "distro": {
            "deliveryWithInstall": true,
            "moduleName": "entry",
            "moduleType": "entry",
            "installationFree": false
        },
        "abilities": [
            {
                "name": "com.huawei.codelab.MainAbility",
                "description": "$string:mainability_description",
                "icon": "$media:icon",
                "label": "$string:NewsClient",
                "type": "page",
                "launchType": "standard",
                "visible": true,
                "skills": [
                    {
                        "actions": [
                            "action.system.home"
                        ],
                        "entities": [
                            "entity.system.home"
                        ]
                    }
                ],
                "configChanges": [
                    "orientation"
                ],
                "homeAbility": true
            }
        ],
        "js": [
            {
                "name": "default",
                "pages": [
                    "pages/NewsMain",
                    "pages/NewsDetail"
                ],
                "window": {
                    "designWidth": 720,
                    "autoDesignWidth": false
                },
                "mode": {
                    "syntax": "ets",
                    "type": "pageAbility"
                }
            }
        ],
        "reqPermissions": [
            {
                "name": "ohos.permission.DISTRIBUTED_DATASYNC",
                "reason": "$string:distributed_datasync_reason",
                "usedScene": {
                    "ability": [
                        ".MainAbility"
                    ],
                    "when": "inuse"
                }
            },
            {
                "name": "ohos.permission.GET_DISTRIBUTED_DEVICE_INFO"
            },
            {
                "name": "com.huawei.permission.ACCESS_SERVICE_DM"
            }
        ],
        "mainAbility": "com.huawei.codelab.MainAbility",
        "distroFilter": {
            "apiVersion": {
                "policy": "include",
                "value": [
                    
                ]
            }
        },
        "package": "com.huawei.codelab"
    }
}